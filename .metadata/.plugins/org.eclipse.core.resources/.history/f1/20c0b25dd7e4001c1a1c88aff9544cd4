package javaPackage;

public class Typecasting_Widening {

	public static void main(String[] args) {
	int x= 40;
	int y = 77;
	double m = x;			 //automatic casting int to double
	double n = y;			//automatic casting int to double
	
	System.out.println(x); 	 // 40
	System.out.println(y); 	 //77
	System.out.println(m); 	//40.0
	System.out.println(n); 	//77.0

	}

}
