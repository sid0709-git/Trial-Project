package javaPackage;

public class Typecasting_Narrowing {

	public static void main(String[] args) {
		double a = 23.14;
		double b = 45.99;
		int d = (int) a ;
		int f = (int)b;
		System.out.println(a);//23.14
		System.out.println(b);//45.99
		System.out.println(d);//23 manuaaly casting double to int     (narrowing of a)
		System.out.println(f);//45 manuaaly casting double to int     (narrowing of b)
	}

}
